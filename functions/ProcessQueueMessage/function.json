{
    "scriptFile": "__init__.py",
    "bindings": [
        {
            "name": "msg",
            "type": "queueTrigger",
            "direction": "in",
            "queueName": "slidesms-send-queue",
            "connection": "AzureWebJobsStorage",
            "maxDequeueCount": 5
        }
    ],
    "retry": {
        "strategy": "fixedDelay",
        "maxRetryCount": 3,
        "delayInterval": "00:00:10"
    }
}